<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <services>
            <service name="Artech.WcfServices.Service.TimeService">
                <endpoint binding="webHttpBinding"
                          contract="Artech.WcfServices.Service.Interface.ITime" />
            </service>
        </services>
      <serviceHostingEnvironment aspNetCompatibilityEnabled="true" />
    </system.serviceModel>  
  <system.web>
    <caching>
      <outputCacheSettings>
        <outputCacheProfiles>
          <add name="default" duration="60" varyByParam="none"/>
        </outputCacheProfiles>
      </outputCacheSettings>      
    </caching>
  </system.web>
</configuration>